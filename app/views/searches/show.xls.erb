<?xml version="1.0"?>
<Workbook xmlns="urn:schemas-microsoft-com:office:spreadsheet"
xmlns:o="urn:schemas-microsoft-com:office:office"
xmlns:x="urn:schemas-microsoft-com:office:excel"
xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet"
xmlns:html="http://www.w3.org/TR/REC-html40">
<Worksheet ss:Name="Sheet1">
  <Table>
    <Row>
      <Cell><Data ss:Type="String">Query: </Data></Cell>
    </Row>
    <Row>
      <Cell><Data ss:Type="String">Language</Data></Cell>
      <Cell><Data ss:Type="String">Language Family</Data></Cell>
      <Cell><Data ss:Type="String">ISO</Data></Cell>
      <Cell><Data ss:Type="String">Area</Data></Cell>
      <Cell><Data ss:Type="String">Country</Data></Cell>
      <Cell><Data ss:Type="String">Region</Data></Cell>
      <Cell><Data ss:Type="String">Inventory</Data></Cell>
      <Cell><Data ss:Type="String">Consonant Number</Data></Cell>
      <Cell><Data ss:Type="String">Vowel Number</Data></Cell>
      <Cell><Data ss:Type="String">Total Number</Data></Cell>
      <Cell><Data ss:Type="String">Result</Data></Cell>
      <Cell><Data ss:Type="String">Time</Data></Cell>
    </Row>
    <Row>
      <Cell><Data ss:Type="String"><%= @search.language_name %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.language_family %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.iso %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.area %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.country %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.region %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.inv %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.c_compare if @search.c_size.present? %> <%= @search.c_size %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.v_compare if @search.v_size.present? %> <%= @search.v_size %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.total_compare if @search.total_size.present? %> <%= @search.total_size %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.papuas.size %>/<%= Papua.all.size %> = <%= @search.result %></Data></Cell>
      <Cell><Data ss:Type="String"><%= @search.created_at.localtime %></Data></Cell>
    </Row>

    <Row>
      <Cell><Data ss:Type="String">Result: <%= @search.papuas.size %> </Data></Cell>
    </Row>
    <Row>
      <Cell><Data ss:Type="String">No.</Data></Cell>
      <Cell><Data ss:Type="String">Language</Data></Cell>
      <Cell><Data ss:Type="String">Language Family</Data></Cell>
      <Cell><Data ss:Type="String">ISO</Data></Cell>
      <Cell><Data ss:Type="String">Country</Data></Cell>
      <Cell><Data ss:Type="String">Latitude</Data></Cell>
      <Cell><Data ss:Type="String">Longitude</Data></Cell>
      <Cell><Data ss:Type="String">Inventories</Data></Cell>
      <Cell><Data ss:Type="String">Consonants</Data></Cell>
      <Cell><Data ss:Type="String">Vowels</Data></Cell>
      <Cell><Data ss:Type="String">Diphthongs</Data></Cell>
      <Cell><Data ss:Type="String">Source</Data></Cell>
      <Cell><Data ss:Type="String">Notes</Data></Cell>
    </Row>
    <% @search.papuas.each do |papua| %>
    <Row>
      <Cell><Data ss:Type="String"><%= papua.no %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.language_name %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.language_family %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.iso %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.country %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.latitude %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.longitude %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.inv %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.consonants %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.v_segments %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.diphthongs %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.source %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.notes %></Data></Cell>
    </Row>
    <% end %>
    <Row>
      <Cell><Data ss:Type="String">Rest: <%= @search.papuas_other.size %> </Data></Cell>
    </Row>
    <% @search.papuas_other.each do |papua| %>
    <Row>
      <Cell><Data ss:Type="String"><%= papua.no %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.language_name %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.language_family %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.iso %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.country %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.latitude %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.longitude %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.inv %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.consonants %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.v_segments %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.diphthongs %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.source %></Data></Cell>
      <Cell><Data ss:Type="String"><%= papua.notes %></Data></Cell>
    </Row>
    <% end %>
  </Table>
</Worksheet>
</Workbook>